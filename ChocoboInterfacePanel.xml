<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

<Frame name="ChocoboInterfacePanel">
	<Scripts>
		<OnLoad>ChocoboInterfacePanel_OnLoad(self);</OnLoad>
	</Scripts>
	<Frames>
		<Button name="$parent_OpenOptionsButton" text="Open Chocobo options" inherits="OptionsButtonTemplate">
			<Size>
				<AbsDimension x="200" y="25" />
			</Size>
			<Anchors>
				<Anchor point="TOPLEFT">
					<Offset>
						<AbsDimension x="20" y="-20" />
					</Offset>
				</Anchor>
			</Anchors>
			<Scripts>
				<OnClick>ChocoboInterfacePanel_OpenOptions();</OnClick>
			</Scripts>
		</Button>
	</Frames>
</Frame>
</Ui>
